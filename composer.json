{
    "name": "feral/symfony",
    "description": "This package provides a symfony bundle to make Feral available as a symfony package.",
    "keywords": ["symfony", "bundle", "bpms", "feral"],
    "homepage": "https://feralflow.net",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "feral/core": "^1.0.48"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Feral\\Symfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feral\\Symfony\\Tests\\": "tests/"
        }
    },
    "replace": {
    },
    "scripts": {
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    }
}
